{
  "name": "medusa-file-r2",
  "version": "0.0.2",
  "type": "module",
  "description": "Cloudflare R2 storage plugin for Medusa",
  "author": "Yinka Adedire <yinkakun@gmail.com>",
  "license": "MIT",
  "scripts": {
    "bundle": "esbuild src/services/** --outdir=services --format=cjs --target=esnext",
    "dev": "yarn bundle --watch",
    "build": "yarn bundle",
    "prepare": "yarn build",
    "format": "prettier --write .",
    "package": "yarn build && yarn np --no-tests --no-yarn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yinkakun/medusa-file-r2.git"
  },
  "files": [
    "services"
  ],
  "dependencies": {
    "aws-sdk": "^2.1295.0",
    "medusa-core-utils": "^1.1.37",
    "medusa-interfaces": "^1.3.3",
    "typeorm": "^0.2.36"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.3",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "np": "^7.6.3",
    "prettier": "^3.3.3",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "medusa-interfaces": "1.x"
  },
  "keywords": [
    "medusa-plugin",
    "medusa-file-plugin",
    "cloudflare-r2"
  ]
}
